generator client {
  provider = "prisma-client-js"
  output   = "../../../../../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Report {
  reportId   Int       @id @default(autoincrement()) @map("report_id")
  executorId String?   @map("executor_id")
  createdAt  DateTime  @default(now()) @map("created_at")
  publishAt  DateTime  @updatedAt @map("publish_at")

  @@map("reports")
}

model CarModel {
  carId      Int     @id @default(autoincrement()) @map("car_id")
  carModel   String  @default("") @map("car_model")
  executorId String? @map("executor_id")

  @@map("cars_models")
}

