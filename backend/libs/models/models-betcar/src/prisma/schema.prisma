generator client {
  provider = "prisma-client-js"
  output   = "../../../../../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Report {
  reportId                Int                       @id @default(autoincrement()) @map("report_id")
  executorId              String?                   @map("executor_id")
  title                   String                    @default("")
  createdAt               DateTime                  @default(now()) @map("created_at")
  publishAt               DateTime                  @updatedAt @map("publish_at")
  sellerDetails           SellerDetails[]
  carData                 CarData[]
  location                Location[]
  complectationBody       ComplectationBody[]
  complectationWheels     ComplectationWheels[]
  complectationSalon      ComplectationSalon[]
  complectationHeadlights ComplectationHeadlights[]
  complectationMultimedia ComplectationMultimedia[]
  complectationComfort    ComplectationComfort[]
  complectationSafety     ComplectationSafety[]
  complectationProtection ComplectationProtection[]
  complectationOther      ComplectationOther[]
  paintworkInspection     PaintworkInspection[]
  engineCompartmentDamage EngineCompartmentDamage[]
  frontSideDamage         FrontSideDamage[]
  leftSideDamage          LeftSideDamage[]
  backSideDamage          BackSideDamage[]
  trunkDamage             TrunkDamage[]
  rightSideDamage         RightSideDamage[]
  roofDamage              RoofDamage[]
  glassDamage             GlassDamage[]
  discDamage              DiscDamage[]
  interiorDamage          InteriorDamage[]
  exteriorPhoto           ExteriorPhoto[]
  interiorPhoto           InteriorPhoto[]
  roundViewVideo          RoundViewVideo[]
  stoppedEngine           StoppedEngine[]
  runningEngine           RunningEngine[]
  dashboard               Dashboard[]
  testDrive               TestDrive[]
  expertOpinion           ExpertOpinion[]

  @@map("reports")
}

// ** Модели сервиса "Документы" ** //

model SellerDetails {
  sellerDetailsId     Int      @id @default(autoincrement()) @map("seller_details_id")
  reportId            Int      @map("report_id")
  report              Report   @relation(fields: [reportId], references: [reportId], onDelete: Cascade, onUpdate: Cascade)
  refusalOfInspection Boolean  @default(false) @map("refusal_of_inspection")
  linkToAd            String[] @default([""]) @map("link_to_ad")
  name                String   @default("")
  phone               String   @default("")
  carCost             String   @default("") @map("car_cost")
  isBargain           Boolean  @default(false) @map("is_bargain")
  role                String   @default("")
  comment             String   @default("")

  @@map("sellers_details")
}

model CarData {
  carDataId             Int                   @id @default(autoincrement()) @map("car_data_id")
  reportId              Int                   @map("report_id")
  report                Report                @relation(fields: [reportId], references: [reportId], onDelete: Cascade, onUpdate: Cascade)
  vinBodyNumber         VinBodyNumber[]
  frameNumber           FrameNumber[]
  stateNumber           StateNumber[]
  tCP                   TCP[]
  sTS                   STS[]
  carBrand              String                @default("") @map("car_brand")
  carModel              String                @default("") @map("car_model")
  manufactureYear       String                @default("") @map("manufacture_year")
  color                 String                @default("")
  bodyType              String                @default("") @map("body_type")
  enginesType           String                @default("") @map("engines_type")
  gearboxType           String                @default("") @map("gearbox_type")
  driveUnit             String                @default("") @map("drive_unit")
  steeringWheelLocation String                @default("") @map("steerint_wheel_location")
  modification          String                @default("")
  engineNumber          EngineNumber[]
  engineCapacity        String                @default("") @map("engine_capacity")
  power                 String                @default("")
  serviceBook           String                @default("") @map("service_book")
  additionalDocuments   AdditionalDocuments[]

  @@map("cars_datas")
}

model VinBodyNumber {
  vinBodyNumberId Int      @id @default(autoincrement()) @map("vin_body_number_id")
  carDataId       Int      @map("car_data_id")
  carData         CarData? @relation(fields: [carDataId], references: [carDataId], onDelete: Cascade, onUpdate: Cascade)
  vinNumber       String   @default("") @map("vin_number")
  expertConfirm   Boolean  @default(false) @map("expert_confirm")
  numberState     String[] @default([""]) @map("number_state")
  photo           String[] @default([""])

  @@map("vin_body_numbers")
}

model FrameNumber {
  frameNumberId Int      @id @default(autoincrement()) @map("frame_number_id")
  carDataId     Int      @map("car_data_id")
  carData       CarData? @relation(fields: [carDataId], references: [carDataId], onDelete: Cascade, onUpdate: Cascade)
  frameNumber   String   @default("") @map("frame_number")
  expertConfirm Boolean  @default(false) @map("expert_confirm")
  numberState   String[] @default([""]) @map("number_state")
  photo         String[] @default([""])

  @@map("frame_numbers")
}

model StateNumber {
  stateNumberId Int      @id @default(autoincrement()) @map("state_number_id")
  carDataId     Int      @map("car_data_id")
  carData       CarData? @relation(fields: [carDataId], references: [carDataId], onDelete: Cascade, onUpdate: Cascade)
  absent        Boolean  @default(false)
  foreign       Boolean  @default(false)
  stateNumber   String   @default("") @map("state_number")

  @@map("state_numbers")
}

model TCP {
  tCPId     Int      @id @default(autoincrement()) @map("tcp_id")
  carDataId Int      @map("report_id")
  carData   CarData? @relation(fields: [carDataId], references: [carDataId], onDelete: Cascade, onUpdate: Cascade)
  absent    Boolean  @default(false)
  tCPType   String   @default("") @map("tcp_type")
  photo     String[] @default([""])
  comment   String   @default("")

  @@map("tcps")
}

model EngineNumber {
  engineNumberId Int      @id @default(autoincrement()) @map("engine_number_id")
  carDataId      Int      @map("car_data_id")
  carData        CarData? @relation(fields: [carDataId], references: [carDataId], onDelete: Cascade, onUpdate: Cascade)
  expertConfirm  Boolean  @default(false) @map("expert_confirm")
  numberState    String[] @default([""]) @map("number_state")
  photo          String[] @default([""])

  @@map("engine_numbers")
}

model AdditionalDocuments {
  additionalDocumentsId Int      @id @default(autoincrement()) @map("additional_documents_id")
  carDataId             Int      @map("car_data_id")
  carData               CarData? @relation(fields: [carDataId], references: [carDataId], onDelete: Cascade, onUpdate: Cascade)
  comment               String   @default("")
  photo                 String[] @default([""])

  @@map("additional_documents")
}

model STS {
  sTSId     Int      @id @default(autoincrement()) @map("sts_id")
  carDataId Int      @map("report_id")
  carData   CarData? @relation(fields: [carDataId], references: [carDataId], onDelete: Cascade, onUpdate: Cascade)
  absent    Boolean  @default(false)
  photo     String   @default("")

  @@map("sts")
}

model Location {
  locationId Int    @id @default(autoincrement()) @map("location_id")
  reportId   Int    @map("report_id")
  report     Report @relation(fields: [reportId], references: [reportId], onDelete: Cascade, onUpdate: Cascade)
  country    String @default("")
  region     String @default("")
  city       String @default("")
  address    String @default("")
  comment    String @default("")

  @@map("locations")
}

