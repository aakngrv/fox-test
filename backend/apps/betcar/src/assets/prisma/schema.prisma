generator client {
  provider = "prisma-client-js"
  output   = "../../../../../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// ** Модель сервиса "Files"** //

model File {
  fileId           Int    @id @default(autoincrement()) @map("file_id")
  originalFilename String @default("") @map("original_filename")
  hashName         String @default("") @map("hash_name")
  mimetype         String @default("") @map("mimetype")
  path             String @default("")
  size             Int

  @@map("files")
}

// ** Модель сервиса "betcar-users" ** //

model User {
  userId       Int      @id @default(autoincrement()) @map("user_id")
  firstname    String   @default("")
  lastname     String   @default("")
  email        String   @unique @default("")
  city         String   @default("")
  phone        String   @default("")
  passwordHash String   @default("")
  executor     Boolean  @default(false)
  customer     Boolean  @default(false)
  admin        Boolean  @default(false)
  createdAt    DateTime @default(now()) @map("created_at")

  @@map("users")
}

model RefreshToken {
  refreshTokenId Int      @id @default(autoincrement()) @map("refresh_token_id")
  userId         Int      @map("user_id")
  tokenId        Int      @map("token_id")
  createdAt      DateTime @default(now()) @map("created_at")
  expiresIn      DateTime @default(now()) @map("expires_in")

  @@map("refresh_tokens")
}

// ************************************************************************************************ //

// ** Модели заявок ** //

model UserBid {
  bidId     Int      @id @default(autoincrement()) @map("bid_id")
  firstname String   @default("")
  email     String   @default("")
  city      String   @default("")
  phone     String   @default("")
  createdAt DateTime @default(now()) @map("created_at")

  @@map("user_bids")
}

model OrganizatonBid {
  bidId             Int      @id @default(autoincrement()) @map("bid_id")
  firstname         String   @default("")
  email             String   @default("")
  organizationName  String   @default("") @map("organization_name")
  city              String   @default("")
  phone             String   @default("")
  createdAt         DateTime @default(now()) @map("created_at")

  @@map("organization_bids")
}

model UserProfile {
  userProfileId           Int      @id @default(autoincrement()) @map("user_profile_id")
  firstname               String   @default("")
  email                   String   @default("")
  age                     String   @default("")
  city                    String   @default("")
  phone                   String   @default("")
  driverLicense           String[] @default([""]) @map("driver_license")
  experienceWithTransport String[] @default([""]) @map("experience_with_transport")
  workExperience          String   @default("") @map("work_experience")
  createdAt               DateTime @default(now()) @map("created_at")

  @@map("user_profiles")
}

model TurnkeySelection {
  turnkeySelectionId Int      @id @default(autoincrement()) @map("turnkey_selection_id")
  firstname          String   @default("")
  phone              String   @default("")
  autoModel          String   @default("")  @map("auto_model")
  mileage            String[] @default([""])
  gearboxType        String[] @default([""]) @map("gearbox_type")
  driveType          String[] @default([""]) @map("drive_type")
  yearOfIssue        String[] @default([""]) @map("year_of_issue")
  colors             String[] @default([""]) 
  engineType         String[] @default([""]) @map("engine_type")
  linkToAd           String   @default("")  @map("link_to_ad")
  createdAt    DateTime  @default(now()) @map("created_at")

   @@map("turnkey_selections")
}

model TurnkeySelectionManually {
  turnkeySelectionManuallyId Int      @id @default(autoincrement()) @map("turnkey_selection_manually_id")
  firstname                  String   @default("")
  phone                      String   @default("")
  autoModel                  String   @default("") @map("auto_model")
  autoMark                   String   @default("") @map("auto_mark")
  yearOfIssue                String[] @default([""]) @map("year_of_issue")
